<!--
   Shortcut to invoke the build script with the compile-resources target.
   Use this to recompile UI plugin resources from Eclipse, if anything changed,
   since the Flex compiler will only build the main .swc file.
-->
<project name="vic-ui-resources" default="compile-resources">
   <target name="compile-resources">
       <ant antfile="build-flex.xml" target="compile-resources"/>
   </target>
</project>