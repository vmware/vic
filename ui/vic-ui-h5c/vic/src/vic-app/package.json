  "main": "index.js",
  "scripts": {
    "rimraf": "rimraf",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "ngc": "ngc",
    "build:dev": "npm run rimraf -- ../main/webapp/resources/build-dev && webpack --env.is_production=0 --progress --display-error-details --display-cached --profile --colors --config ./config/webpack.config.js --bail",
    "build:prod": "npm run rimraf -- ../main/webapp/resources/dist && webpack --env.is_production=1 --display-error-details --colors --config ./config/webpack.config.js --bail",
    "build:prod_aot": "npm run rimraf -- ../main/webapp/resources/dist && ngc -p tsconfig.aot.json && webpack --env.is_aot=1 --env.is_production=1 --colors --display-error-details --config ./config/webpack.config.js --bail",
    "server:dev": "webpack-dev-server --config ./config/webpack.config.js --profile --colors --display-error-details --content-base ../main/webapp/resources/build-dev --inline --bail",
    "server:standalone": "webpack-dev-server --config ./config/webpack.config.js --env.is_standalone=1 --profile --colors --display-error-details --content-base ../main/webapp/resources/build-dev --inline --bail",
    "test": "karma start config/karma.conf.js"
  },
  "dependencies": {
    "@angular/common": "~2.4.7",
    "@angular/compiler": "~2.4.7",
    "@angular/core": "~2.4.7",
    "@angular/forms": "~2.4.7",
    "@angular/http": "~2.4.7",
    "@angular/platform-browser": "~2.4.7",
    "@angular/platform-browser-dynamic": "~2.4.7",
    "@angular/platform-server": "~2.4.7",
    "@angular/router": "~3.4.7",
    "@angular/upgrade": "~2.4.7",
    "@types/lodash": "4.14.39",
    "@webcomponents/custom-elements": "github:webcomponents/custom-elements#v1.0.0-alpha.3",
    "clarity-angular": "0.8.6",
    "clarity-icons": "0.8.6",
    "clarity-ui": "0.8.6",
    "core-js": "2.4.1",
    "jquery": "3.1.0",
    "lodash": "4.17.2",
    "mutationobserver-shim": "0.3.2",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.1",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~2.4.7",
    "@types/core-js": "0.9.35",
